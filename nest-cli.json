{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-bolierplate/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestjs-bolierplate/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjs-bolierplate",
  "projects": {
    "admin-api": {
      "type": "application",
      "root": "apps/admin-api",
      "entryFile": "main",
      "sourceRoot": "apps/admin-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-api/tsconfig.app.json"
      }
    },
    "front-api": {
      "type": "application",
      "root": "apps/front-api",
      "entryFile": "main",
      "sourceRoot": "apps/front-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/front-api/tsconfig.app.json"
      }
    },
    "batch": {
      "type": "application",
      "root": "apps/batch",
      "entryFile": "main",
      "sourceRoot": "apps/batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/batch/tsconfig.app.json"
      }
    }
  }
}